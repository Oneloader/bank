html,body{font-family:"microsoft yahei";margin:0px; padding:0px; background: #eeeeee}

h1,h2,h3,h4,h5,h6,p{margin:0px; padding:0px;}
a,a:hover,a:visited,a:focus{text-decoration:none; color:#333;}
ul,ol{margin: 0px;padding: 0px;list-style-type: none;}
b,strong{font-weight:bold;}
i,em{font-style:normal;}
.clearfix {  zoom: 1 }
.clearfix:after {clear: both;display: block;visibility: hidden;overflow: hidden;height: 0;content: "&nbsp;";zoom: 1;}


.center_box{position: fixed;width: 100%;height: 100%;top: 0px;left: 0px;background: rgba(0,0,0,0.5);z-index: 100;display: none;}
.center_box>div{position: relative;width: 100%;height: 100%;}
.center_box>div>div{display: table;width: 100%;height: 100%;}
.center_box>div>div>div{display: table-cell;vertical-align: middle;}
.contents-box{width: 80%;margin: 0 auto;background: #ffffff;border-radius: 5px;
    padding:10px 20px;}
.login_box{padding-top: 100px;}
.login_logo{width: 55%;margin: 0 auto;display: block}
.login_ipt{width: 70%;margin: 120px auto 0px;}
.code_ipt{margin-top: 20px;}
.ipt_login{padding: 5px 10px;border: 1px solid #154c93;border-radius: 5px;background: #ffffff;width: 100%;outline: none;font-size: 14px;line-height: 25px;}
.ipt_code{padding: 5px 10px;border: 1px solid #154c93;border-radius: 5px;background: #ffffff;width: 48%;outline: none;font-size: 14px;line-height: 25px;}
.btn_blue{width: 48%;background: #154c93;color: #ffffff;border-radius: 5px;border: none;outline: none;line-height: 25px;padding: 6px 0px;text-align: center;font-size: 14px;}
.btn_orange{width: 100%;background: #ffaa00;color: #ffffff;border-radius: 5px;border: none;outline: none;line-height: 25px;padding: 6px 0px;text-align: center;margin-top: 20px;display: block;}
.btn_blue.send{background: #999999;}

.head_menu{background: #154c93;height: 65px;padding: 10px;position: fixed;top: 0px;left: 0px;width: 100%;z-index: 99;}
.top_logo{height: 100%;width: auto;}
.manage_names{padding-left: 10px;}
.top_name{font-size: 18px;color: #ffffff;line-height: 45px;}
.righr_menu{text-align: center;height: 45px;}
.righr_menu i{display: block;color: #ffffff;line-height: 28px;}
.righr_menu span{display: block;color: #ffffff;font-size: 12px;}
.icon-gift{font-size: 24px;}
.icon-fanhui2{font-size: 24px;}
.icon-tuichu{font-size: 24px;}
.gift{margin-right: 10px;}
.activity_list{padding: 10px;}
.activity_list_li{background: #ffffff;padding: 5px;margin-bottom: 10px;}
.activity_list_li:last-of-type{margin-bottom: 0px;}
.list_link{position: relative;display: block;}
.list_link img{width: 100%;margin-top: 10px;}
.item_title span{font-size: 16px;color: #353535;line-height: 25px;}
.item_title em{font-size: 12px;color: #999999;line-height: 25px;}
.activity_status{position: absolute;left: 0px;bottom: 0px;padding: 5px 10px;font-size: 12px;letter-spacing: 5px;}
.running{background: #11b500;color: #ffffff;}
.tips_text{font-size: 12px;text-align: center;margin: 15px 0px;color: #999999;}
.page_content{padding-top: 65px;}



.activity_name{font-size: 12px;color: #ffffff;display: block;line-height: 14px;}
.activity_in{line-height: 28px;}
.tab_menu{background: #ffffff;position: fixed;top: 65px;left: 0px;width: 100%;z-index: 10;}
.menu_item{width: 50%;text-align: center;display: block;border-bottom: 2px solid #ffffff;line-height: 30px;font-size: 14px;color: #353535}
.menu_item.active{border-bottom: 2px solid #154c93;color: #154c93}
.data_content{padding-top: 97px;}
.data_li{background: #ffffff;padding:5px 10px;margin-top: 10px;position: relative;}
.li_data{font-size: 14px;color: #353535;}
.li_data em{font-size: 12px;color: #999999;margin-left: 10px;}
.icon-next{position: absolute;top: 8px;right: 8px;font-size: 12px;
    transition: all ease 0.5s;
    -webkit-transition: all ease 0.5s;
    -moz-transition: all ease 0.5s;
    -o-transition: all ease 0.5s;}
.order_list{margin: 0px;margin-top: 5px;list-style-type: none;}
.order_list_li{border-top: 1px solid #dddddd;padding: 10px 0px;}
.order_names p{font-size: 14px;color: #353535;line-height: 24px;display: block}
.hui_text{font-size: 12px;color: #999999;display: block;line-height: 24px;}
.order_right a{font-size: 14px;color: #154c93;line-height: 24px;display: block}
.order_list_li img{width: 50px;margin-right: 10px;}


.info_list_li{background: #ffffff;margin-top: 10px;}
.order_title{padding: 8px;border-bottom: 1px solid #dddddd}
.order_title span{font-size: 12px;color: #154c93}
.order_title em{font-size: 12px;color: #999999}
.order_shop{padding: 10px;}
.order_shop img{width: 50px;float: left;margin-right: 10px;}
.order_shop p{float: left;font-size: 14px;color: #353535;line-height: 50px;}
.order_shop span{float: right;font-size: 12px;color: #999999;line-height: 50px;}
.other_text{font-size: 12px;color: #353535;padding: 8px;border-top: 1px solid #dddddd;}
.looking_gift{color: #154c93!important;border-radius: 5px;border: 1px solid #154c93;font-size: 12px;padding:0px 5px;}
.other_text em{color: #11b500}


.top_bgs{position: relative;}
.top_bgs img{width: 100%;}
.text_img{position: absolute;width: 60%!important;top: 30%;left: 20%;}
.shop_list{background: url(../images/background.png) repeat;padding-bottom: 15px;}
.shop_list_box{padding: 20px;}
.shop_list_box>li{width: 48%;float: left;background: #ffffff;padding: 10px;margin-bottom: 15px;border-radius: 5px;-moz-box-shadow:5px 5px 20px #000000; -webkit-box-shadow:5px 5px 20px #000000; box-shadow:5px 5px 20px #000000;}
.shop_list_box>li:nth-of-type(odd){margin-right: 2%;}
.shop_list_box>li:nth-of-type(even){margin-left: 2%;}
.shop_list_box>li a{display: block;}
.shop_list_box>li img{width: 100%;}
.shop_name{font-size: 14px;color: #353535;height: 44px;}
.shop_list_box>li span{display: block;color: #999999;font-size: 12px;}
.shop_list_box>li em{display: block;color: #999999;font-size: 12px;}
.shop_list_box>li i{color: #ff0000;font-size: 12px;}
.shop_info_box{width: 90%;margin: 0px auto 20px;background: #ffffff;padding: 10px;border-radius: 5px;}
.button_dui{width: 90%;margin: 0 auto;}
.button_dui .btn_orange{margin: 0px;}
.shop_info_box img{max-width: 100%;display: inline-block;}
.input_form{width: 90%;margin: 0px auto 20px;background: #ffffff;padding: 15px;border-radius: 5px;}



.shop_numbers img{width: 70px;float: left;margin-right: 10px;border: 1px solid #154c93;}
.add_num{margin-top: 15px;}
.add_num em{font-size: 14px;color: #353535;float: left;}
.names{font-size: 14px;color: #353535;}
.num_down{border: 1px solid #154c93;width: 22px;float: left;height: 22px;text-align: center;line-height: 19px;font-size: 16px;}
.shop_num{border: 1px solid #154c93;padding:0px 15px;font-size: 14px;color: #353535;float: left;height: 22px;margin: 0px 5px;}
.num_up{border: 1px solid #154c93;float: left;height: 22px;width: 22px;text-align: center;line-height: 19px;font-size: 16px;}
.ipt_item{position: relative;padding-left: 80px;margin-top: 20px;}
.ipt_item_text{position: absolute;left: 0px;top: 0px;font-size: 14px;color: #353535;line-height: 32px;}
.ipt_item input{padding: 5px 10px;border: 1px solid #154c93;border-radius: 5px;background: #ffffff;width: 100%;outline: none;font-size: 14px;line-height: 20px;}
.ipt_item select{padding: 5px 10px;border: 1px solid #154c93;border-radius: 5px;background: #ffffff;width: 100%;outline: none;font-size: 14px;line-height: 20px;margin-bottom: 5px;}



.user_pages{position: fixed;width: 100%;height: 100%;top: 0px;left: 0px;overflow: hidden}
.user_pages>.pages_img{width: 100%;}
.user_logo{position: absolute;top: 20%;left: 20%;width: 60%;}
.user_text{position: absolute;top: 40%;left: 20%;width: 60%;}
.user_btn{position: absolute;top: 60%;left: 20%;width: 60%;}




.contents-box>h2{font-size: 16px;color: #353535;font-weight: normal;text-align: center;
    line-height:40px;margin-bottom: 15px;}
.user_btns{margin-top: 30px;margin-bottom: 10px;}
.user_btns>a{width: 40%;text-align: center;border-radius: 5px;line-height: 30px;font-size: 14px;}
.conform_cancel{border: 1px solid #154c93;color: #999999;}
.conform_btn{background: #154c93;color: #ffffff!important;line-height: 32px!important;}


.order_box{height: 0px;overflow: hidden;
    transition: all ease 0.5s;
    -webkit-transition: all ease 0.5s;
    -moz-transition: all ease 0.5s;
    -o-transition: all ease 0.5s;
}



.data_li.show .icon-next{
    transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    -moz-transform: rotate(90deg);
    -o-transform: rotate(90deg);
}
.data_li.show .order_box{height: auto;}
.refresh{display: none;}
.loadingBox{background: none!important;text-align: center;margin-bottom: 20px;font-size: 12px;color: #999999;}
#app{position: fixed;left: 0;top: 0;width: 100%;height: 100%;overflow-y:auto;-webkit-overflow-scrolling: touch;}

.logistics_hd{background: #ffffff;padding: 10px 15px;}
.logistics_hd p{font-size: 14px;color: #353535;line-height: 25px;}

.logistics_box{margin-top: 15px;}
.logistics_blist{padding:15px 0px 15px 50px;background: #ffffff;position: relative;}
.logistics_blist>li{padding: 10px;border-bottom: 1px solid #dddddd;position: relative}
.logistics_blist>li p{font-size: 14px;color: #999999;}
.logistics_blist>li span{font-size: 12px;color: #999999;}
.logistics_blist>li:first-of-type p{color: #353535;}
.logistics_blist>li em{position: absolute;width: 1px;height: 100%;background: #dddddd;top: 0px;left: -25px;z-index: 3}
.logistics_blist>li i{width: 7px;height: 7px;position: absolute;top: 20px;left: -28px;border-radius: 100%;background-color: #dddddd;z-index: 3;}

.set_pos_line{position: absolute!important;top: 0px;left: 25px;padding: 0px!important;width: 1px;height: 100%;background: #dddddd;z-index: 2}
.logistics_blist>li:first-of-type i{width: 20px;height: 20px;background: #154c93;left: -34px;border: 3px solid #8AA6CB;top: 13px;
}





